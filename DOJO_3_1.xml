      <Task>
        <Name>Generate wrong XML</Name>
        <State>WAITING</State>
        <Type>SCHEDULE</Type>
        <HistorySize>10</HistorySize>
        <ScheduleProperties>
          <Output>
            <Id>TESTTRIG</Id>
            <Name>Testar schedule</Name>
            <MetaData>
              <ITEM>D</ITEM>
              <MAD>jska</MAD>
            </MetaData>
          </Output>
          <TimeFrame>
            <Weekday>ALL</Weekday>
            <Month>ALL</Month>
            <Year>ALL</Year>
            <Day>ALL</Day>
          </TimeFrame>
          <Start>
            <Time>15:43:00</Time>
            <OnLoad>true</OnLoad>
          </Start>
          <Repeat>
            <Enabled>true</Enabled>
            <Times>-1</Times>
            <IntervalMS>1020</IntervalMS>
          </Repeat>
        </ScheduleProperties>
        <Task>
          <Name>Check XML</Name>
          <State>WAITING</State>
          <Type>VALIDATE</Type>
          <HistorySize>10</HistorySize>
          <ValidateProperties>
            <Id>MAM</Id>
            <Name>Mommy</Name>
            <XSDFileName>xsd\dojo.xsd</XSDFileName>
            <OutPutError>true</OutPutError>
            <SkipDuplicates>
              <Enabled>true</Enabled>
            </SkipDuplicates>
          </ValidateProperties>
          <Task>
            <Name>Correct XML</Name>
            <State>WAITING</State>
            <Type>WRITER</Type>
            <HistorySize>10</HistorySize>
            <ValidateInput>
              <Check>
                <XPath>/*[name(.) ="VALIDATE_ERROR"]</XPath>
                <Result>0</Result>
                <Type>COUNT</Type>
                <Condition>AND</Condition>
              </Check>
              <Enabled>false</Enabled>
            </ValidateInput>
            <WriterProperties>
              <Channel>420</Channel>
              <Inherit>true</Inherit>
            </WriterProperties>
          </Task>
          <Task>
            <Name>Wrong XML</Name>
            <State>WAITING</State>
            <Type>WRITER</Type>
            <HistorySize>10</HistorySize>
            <ValidateInput>
              <Check>
                <XPath>/*[name(.) ="VALIDATE_ERROR"]</XPath>
                <Result>1</Result>
                <Type>COUNT</Type>
                <Condition>AND</Condition>
              </Check>
              <Enabled>false</Enabled>
            </ValidateInput>
            <WriterProperties>
              <Channel>421</Channel>
              <Inherit>true</Inherit>
            </WriterProperties>
          </Task>
        </Task>
      </Task>
    </Task>
