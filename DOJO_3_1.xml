    </Properties>
    <Task>
      <Name>1 second interval</Name>
      <State>WAITING</State>
      <Type>SCHEDULE</Type>
      <HistorySize>10</HistorySize>
      <ScheduleProperties>
        <Output>
          <Id>TESTTRIG</Id>
          <Name>Test data</Name>
          <MetaData>
            <ITEM>D</ITEM>
            <DATA>
              <ID>D1</ID>
              <VALUE>Bla bla bla</VALUE>
            </DATA>
          </MetaData>
        </Output>
        <TimeFrame>
          <Weekday>ALL</Weekday>
          <Month>ALL</Month>
          <Year>ALL</Year>
          <Day>ALL</Day>
        </TimeFrame>
        <Start>
          <Time>15:43:00</Time>
          <OnLoad>true</OnLoad>
        </Start>
        <Repeat>
          <Enabled>true</Enabled>
          <Times>-1</Times>
          <IntervalMS>1000</IntervalMS>
        </Repeat>
      </ScheduleProperties>
      <Task>
        <Name>Create Insert XML</Name>
        <State>WAITING</State>
        <Type>PROCESS</Type>
        <HistorySize>10</HistorySize>
        <ProcessProperties>
          <Inherit>true</Inherit>
          <Xslt>DB_INSERT_TEST.xsl</Xslt>
        </ProcessProperties>
        <Task>
          <Name>Insert to DB</Name>
          <State>WAITING</State>
          <Type>SAVE_TO_DATABASE</Type>
          <HistorySize>10</HistorySize>
          <SaveToDatabaseProperties>
            <Inherit>true</Inherit>
            <Plugin>XmlDBCorePlugin.XmlDBDynamicInsert</Plugin>
          </SaveToDatabaseProperties>
        </Task>
      </Task>
    </Task>
    <Task>
      <Name>10 second interval</Name>
      <State>WAITING</State>
      <Type>SCHEDULE</Type>
      <HistorySize>10</HistorySize>
      <ScheduleProperties>
        <Output>
          <Id>EMPTY</Id>
          <Name>Empty trig</Name>
        </Output>
        <TimeFrame>
          <Weekday>ALL</Weekday>
          <Month>ALL</Month>
          <Year>ALL</Year>
