        </ProcessProperties>
        <Task>
          <Name>Delete From DB</Name>
          <State>WAITING</State>
          <Type>DELETE_FROM_DATABASE</Type>
          <HistorySize>10</HistorySize>
          <DeleteFromDatabaseProperties>
            <Inherit>true</Inherit>
            <Plugin>XmlDBCorePlugin.XmlDBDynamicDelete</Plugin>
          </DeleteFromDatabaseProperties>
        </Task>
      </Task>
    </Task>
  </Task>
  <Task>
