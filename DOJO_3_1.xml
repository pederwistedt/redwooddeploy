  <Task>
    <Name>QueueTest</Name>
    <State>WAITING</State>
    <Type>GROUP</Type>
    <Properties>
      <Ip>127.0.0.1</Ip>
      <RouterPort>8890</RouterPort>
      <DatabasePort>1234</DatabasePort>
      <Database>Prod</Database>
      <DatabaseVersion>1.6</DatabaseVersion>
      <DataRouterVersion>1.1</DataRouterVersion>
    </Properties>
    <Task>
      <Name>Schedule Without Command 3 sec</Name>
      <State>WAITING</State>
      <Type>SCHEDULE</Type>
      <HistorySize>10</HistorySize>
      <ScheduleProperties>
        <Output>
          <Id>TESTTRIG</Id>
          <Name>Queue utan command 3 sek</Name>
          <SendMetaDataOnly>true</SendMetaDataOnly>
          <MetaData>
            <NoCommand>
              <Row1>hoho</Row1>
              <Row2>Santa Claus</Row2>
            </NoCommand>
          </MetaData>
        </Output>
        <TimeFrame>
          <Weekday>ALL</Weekday>
          <Month>ALL</Month>
          <Year>ALL</Year>
          <Day>ALL</Day>
        </TimeFrame>
        <Start>
          <Time>15:43:00</Time>
          <OnLoad>true</OnLoad>
        </Start>
        <Repeat>
          <Enabled>true</Enabled>
          <Times>-1</Times>
          <IntervalMS>3000</IntervalMS>
        </Repeat>
      </ScheduleProperties>
      <Task>
        <Name>Schedule with CLEAR_AND_ADD command 30 sec</Name>
        <State>WAITING</State>
        <Type>SCHEDULE</Type>
        <HistorySize>10</HistorySize>
        <ScheduleProperties>
          <Output>
            <Id>TESTTRIG</Id>
            <Name>Queue utan command 3 sek</Name>
            <SendMetaDataOnly>true</SendMetaDataOnly>
            <MetaData>
              <ClearAndAdd>
                <Row1>hoho... arrghh</Row1>
                <Row2>Santa Claus Died</Row2>
                <COMMAND>CLEAR_AND_ADD</COMMAND>
              </ClearAndAdd>
            </MetaData>
          </Output>
          <TimeFrame>
            <Weekday>ALL</Weekday>
            <Month>ALL</Month>
            <Year>ALL</Year>
            <Day>ALL</Day>
          </TimeFrame>
          <Start>
            <Time>15:43:00</Time>
            <OnLoad>true</OnLoad>
          </Start>
          <Repeat>
            <Enabled>true</Enabled>
            <Times>-1</Times>
            <IntervalMS>30000</IntervalMS>
          </Repeat>
        </ScheduleProperties>
        <Task>
          <Name>Queue</Name>
          <State>WAITING</State>
          <Type>GROUP</Type>
          <Properties>
            <InHerit>true</InHerit>
            <Queue>
              <Enabled>true</Enabled>
              <SendStatus>false</SendStatus>
              <AutoStart>true</AutoStart>
              <DefaultCommand>
                <Enabled>true</Enabled>
                <Command>
                  <COMMAND>ADD</COMMAND>
                  <WAIT>00:00:05:00</WAIT>
                </Command>
              </DefaultCommand>
            </Queue>
          </Properties>
        </Task>
      </Task>
    </Task>
