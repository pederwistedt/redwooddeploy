        </Start>
        <Repeat>
          <Enabled>true</Enabled>
          <Times>-1</Times>
          <IntervalMS>10000</IntervalMS>
        </Repeat>
      </ScheduleProperties>
      <Task>
        <Name>Create Delete XML</Name>
